
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}


model User {
  id       String  @id @default(uuid())
  name     String
  nickname String  @unique
  email    String  @unique
  password String
  cpf      String  @unique
  isAdmin  Boolean @default(false)
  createAt DateTime @default(now())
  updateAt DateTime @updatedAt

}

